
@import './element-ui.css';
:root {
    --main-color: #43B3B4;
    --main-hover: #1091A9;
}
/*滚动条样式*/
::-webkit-scrollbar {
    width: 5px;
    height: 6px;
    background-color: rgba(67,179,180, 0);
}
::-webkit-scrollbar-track {
    background-color: rgba(0, 98, 255, .1);
    border-radius: 2px;
}
::-webkit-scrollbar-thumb {
    border-radius: 10px;
    height: 4px;
    background-color: rgba(0, 98, 255, .5);
}


