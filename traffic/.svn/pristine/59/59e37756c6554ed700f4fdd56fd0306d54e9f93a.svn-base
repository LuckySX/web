{
  "workbench.colorTheme": "Monokai",
  "editor.tabSize": 2, // 2缩进
  "vetur.format.defaultFormatter.html": "js-beautify-html", // 安装vetur插件
  "vetur.format.defaultFormatter.js": "vscode-typescript",
  "files.autoSave": "off",
  "editor.detectIndentation": false, // //根据文件后缀名定义vue文件类型
  "files.associations": {
    "*.vue": "vue"
  }, //配置 ESLint 检查的文件类型
  "eslint.validate": [
    "javascript",
    "javascriptvue",
    {
      "language": "vue",
      "autoFix": true
    }
  ], //保存自动格式化
  "editor.formatOnSave": true,
  "fileheader.customMade": {
    //此为头部注释
    // "autoAdd": true,
    "Author": "fuping",
    "Date": "Do not edit",
    "LastEditors": "fuping",
    "LastEditTime": "Do not edit",
    "Description": ""
  },
  "fileheader.cursorMode": {
    //此为函数注释
    "name": "",
    "param": "",
    "return": "",
    "description": ""
  },
  "window.zoomLevel": 1,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
